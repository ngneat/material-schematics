{
  "name": "@ngneat/material-schematics",
  "version": "1.0.0-beta.2",
  "description": "A blank schematics",
  "publishConfig": {
    "access": "public"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ngneat/material-schematics/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ngneat/material-schematics"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "scripts": {
    "commit": "cz",
    "start": "npm run build -- -w",
    "build": "tsc -p tsconfig.json",
    "test": "jasmine src/**/*_spec.js",
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.{ts,js}\" && prettier \"src/**/*.{ts,md,json,js}\" --check"
  },
  "keywords": [
    "schematics",
    "material",
    "angular",
    "sass",
    "scss",
    "theme"
  ],
  "author": {
    "email": "shhdharmen@gmail.com",
    "name": "Dharmen Shah",
    "url": "https://shhdharmen.me"
  },
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^13.1.1",
    "@angular-devkit/schematics": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "inquirer": "^8.2.0",
    "typescript": "~4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@schematics/angular": "^13.1.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/bluebird": "^3.5.36",
    "@types/color-convert": "^2.0.0",
    "@types/inquirer": "^8.1.3",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jasmine": "^3.5.0",
    "lint-staged": "^12.1.2",
    "prettier": "2.5.1",
    "semantic-release": "^18.0.1"
  },
  "peerDependencies": {
    "@angular/material": "^13.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "eslint --cache --fix",
    "src/**/*.{ts,md,json,js}": "prettier --write"
  }
}
